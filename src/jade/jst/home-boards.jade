ul.pure-g
  each board in boards
    li.board-li.lazy-box.pure-u-1-2
      - var classes = []
      if !board.href
        - classes.push('default')
      a(href=board.href||false, class=classes)
        //- board min-height 100px
        .lazy-box.unloaded
          img.lazy(alt=board.alt||'', data-original=board.src||false)
